# For local config
Include ~/.ssh/config.local

Host *
    AddKeysToAgent yes

Host github.com
    Hostname ssh.github.com
    Port 443
    User git

Host pi-local
    HostName raspberrypi.local
    User zrx
    Port 3333
    RemoteForward 9876 127.0.0.1:7890
    ForwardAgent yes

Host pi
    HostName 166.111.238.16
    User zrx
    Port 3333
    ForwardAgent yes
